        id="cmd_emmet_split_join_tag"
        key="key_cmd_emmet_split_join_tag"
        desc="Emmet: Split/Join Tag"
        oncommand="runEmmetAction('split_join_tag');"/>
      <broadcaster
        id="cmd_emmet_toggle_comment"
        key="key_cmd_emmet_toggle_comment"
        desc="Emmet: Toggle Comment"
        oncommand="runEmmetAction('toggle_comment');"/>

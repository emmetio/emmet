        id="cmd_emmet_decrement_1"
        key="key_cmd_emmet_decrement_1"
        desc="Emmet: Decrement Number By 1"
        oncommand="runEmmetAction('decrement_number_by_1');"/>
      <broadcaster
        id="cmd_emmet_increment_01"
        key="key_cmd_emmet_increment_01"
        desc="Emmet: Increment Number By 0.1"
        oncommand="runEmmetAction('increment_number_by_01');"/>
      <broadcaster
        id="cmd_emmet_decrement_01"
        key="key_cmd_emmet_decrement_01"
        desc="Emmet: Decrement Number By 0.1"
        oncommand="runEmmetAction('decrement_number_by_01');"/>
      <broadcaster
        id="cmd_emmet_increment_10"
        key="key_cmd_emmet_increment_10"
        desc="Emmet: Increment Number By 10"
        oncommand="runEmmetAction('increment_number_by_10');"/>
